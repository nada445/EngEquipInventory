<!DOCTYPE html>
<html>
  <head>
    <head>
      <link rel='stylesheet' href='/styles/bootstrap.min.css' />
      <link rel='stylesheet' href='/styles/style.css' />
      <script src="/js/jquery-2.2.0.min.js" type="text/javascript"></script>
      <script src="/js/bootstrap.min.js" type="text/javascript"></script>
      <script src="/src/cart.js" type="text/javascript"></script>
    </head>
  </head>
  <body>
    <div>
       <nav class="navbar navbar-inverse" style="padding-left:130px;">
          <ul class="nav navbar-nav">
            <li ><a href="/dashboard">Home </a> </li>
            <li><a href="/dashboard"> equipments </a></li>
            <li class="active"><a href="/cart"> cart</a> </li>
          </ul>
      </nav>
    </div>

    <div class="container">
      <table class="table table-striped">
        <thead>
          <tr>
            <th class="text-center">equipment image</th>
            <th class="text-center">equipment name</th>
            <th class="text-center">quantity</th>
          </tr>
        </thead>
        <tbody id="tbody">

          {{#cartitems}}
            <tr>
                <td class="text-center">{{id}}</td>
                <td class="text-center">{{firstname}}</td>
                <td class="text-center">{{middlename}}</td>
                <td class="text-center">{{lastname}}</td>
                <td class="text-center">{{country}}</td>
                <td class="text-center">{{salary}}</td>
                <td class="text-center">
                    <button id={{id}} class="btn btn-danger remove" type="button">Delete</button>
                </td>           
            </tr>
          {{/emp}}
          
        </tbody>
      </table>
    </div>

  </body>
</html>
